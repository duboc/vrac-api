            "in": "header"
        }
    },
    "definitions": {
        "OnboardingPlanRequest": {
            "properties": {
                "name": {
                    "type": "string",
                    "description": "Plan name."
                },
                "description": {
                    "type": "string",
                    "description": "Plan description."
                },
                "projectId": {
                    "type": "string",
                    "description": "Project identifier the plan will onboard machines to."
                },
                "endpointIds": {
                    "type": "array",
                    "description": "Cloud account identifiers to lookup for discovered machines.",
                    "items": {
                        "type": "string"
                    }
                }
            }
        },
        "OnboardingPlanResponse": {
            "properties": {
                "name": {
                    "type": "string",
                    "description": "Plan name."
                },
                "description": {
                    "type": "string",
                    "description": "Plan description."
                },
                "projectId": {
                    "type": "string",
                    "description": "Project identifier the plan will onboard machines to."
                },
                "endpointIds": {
                    "type": "array",
                    "description": "Cloud account identifiers to lookup for discovered machines.",
                    "items": {
                        "type": "string"
                    }
                },
                "tenantLink": {
                    "type": "string",
                    "description": "Link to the plan's tenant."
                },
                "documentSelfLink": {
                    "type": "string",
                    "description": "Link to this plan."
                }
            }
        },
        "OnboardingDeploymentRequest": {
            "properties": {
                "name": {
                    "type": "string",
                    "description": "Deployment name. Will be propagated to Cloud Assembly deployment."
                },
                "description": {
                    "type": "string",
                    "description": "Deployment description. Will be propagated to Cloud Assembly deployment."
                },
                "planLink": {
                    "type": "string",
                    "description": "Link to the parent plan."
                }
            }
        },
        "OnboardingDeploymentResponse": {
            "properties": {
                "name": {
                    "type": "string",
                    "description": "Deployment name. Will be propagated to Cloud Assembly deployment."
                },
                "description": {
                    "type": "string",
                    "description": "Deployment description. Will be propagated to Cloud Assembly deployment."
                },
                "planLink": {
                    "type": "string",
                    "description": "Link to the parent plan."
                },
                "consumerDeploymentLink": {
                    "type": "string",
                    "description": "Link to the onboarded deployment in Cloud Assembly."
                },
                "tenantLink": {
                    "type": "string",
                    "description": "Link to the deployment's tenant."
                },
                "documentSelfLink": {
                    "type": "string",
                    "description": "Link to this deployment."
                }
            }
        },
        "OnboardingResourceRequest": {
            "properties": {
                "resourceLink": {
                    "type": "string",
                    "description": "Link to the compute resource (machine) in the Provisioning service inventory."
                },
                "resourceName": {
                    "type": "string",
                    "description": "Resource name, usually the machine name. Will be propagated to Cloud Assembly."
                },
                "tagLinks": {
                    "type": "array",
                    "description": "Tag links in the Provisioning service inventory associated with the machine.",
                    "items": {
                        "type": "string"
                    }
                },
                "deploymentLink": {
                    "type": "string",
                    "description": "Link to the parent deployment."
                },
                "planLink": {
                    "type": "string",
                    "description": "Link to the parent plan."
                }
            }
        },
        "OnboardingResourceResponse": {
            "properties": {
                "resourceLink": {
                    "type": "string",
                    "description": "Link to the compute resource (machine) in the Provisioning service inventory."
                },
                "resourceName": {
                    "type": "string",
                    "description": "Resource name, usually the machine name. Will be propagated to Cloud Assembly."
                },
                "tagLinks": {
                    "type": "array",
                    "description": "Tag links in the Provisioning service inventory associated with the machine.",
                    "items": {
                        "type": "string"
                    }
                },
                "deploymentLink": {
                    "type": "string",
                    "description": "Link to the parent deployment."
                },
                "planLink": {
                    "type": "string",
                    "description": "Link to the parent plan."
